generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model portafolio {
  grupo_concepto                    String?
  descripcion_grupo_concepto        String?
  concepto_hptu                     String?
  descripcion_concepto              String?
  codigo_reemplazo_cambios_ano_2025 String?
  codigo_hptu                       String  @id
  descripcion_hptu                  String?
  tarifa_2025_bo                    BigInt?
  grupo_quirurgico                  String?
  codigo_conversion_externo         String?
  descripcion_manual_externo        String?
  codigo_cups_asociado              String?
  pbs_no_pbs                        String?
  aseguradora                       String?
  codigo_reps_resolucion_3100_2019  String?
  observacion_a_terceros            String?
  incluye                           String?
  excluye                           String?
  observaciones_generales           String?
}
